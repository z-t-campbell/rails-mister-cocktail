<h1><%= @cocktail.name %></h1>
<ul>
  <% @cocktail.doses.each  do |dose| %>
  <li><%= dose.description%> <%= dose.ingredient.name%> <%= link_to "delete", cocktail_dose_path(@cocktail, dose), {
    method: "delete"
  }%></li>
  <%end%>
</ul>

<p><%= link_to "Add dose", new_cocktail_dose_path(@cocktail)%></p>
<p><%= link_to "Edit", edit_cocktail_path(@cocktail) %></p>
